{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "sing-box-windows",
  "version": "1.8.0",
  "identifier": "cn.moncn.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:6221",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "sing-box-windows",
        "width": 960,
        "height": 640,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "src/config/*"
    ]
  }
}